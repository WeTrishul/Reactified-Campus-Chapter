<section class="text-gray-600 body-font">
    <div class="container px-5 mx-auto max-w-7x1">
      <div class="flex flex-wrap w-full mb-4 p-4">
        <div class="w-full mb-6 lg:mb-0">
        </div>
      </div>
      <div class="flex flex-wrap -m-4">
        <%if(query=='CodeForces'){%>
        <%for( var i of result){%>
        <div class="xl:w-1/3 md:w-1/2 p-4">
          <div class="bg-white p-6 rounded-lg">
            <img class="lg:h-60 xl:h-56 md:h-64 sm:h-72 xs:h-72 h-72 rounded w-full object-cover object-center mb-6" src="https://news.itmo.ru/images/news/big/917925.jpg" alt="Image Size 720x400">
